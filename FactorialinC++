#include<iostream.h>
#include<conio.h>
 
void main()
{
    int n,f=1,i=1;
    clrscr();
     
    cout<<"\n Enter The Number:";
    cin>>n;
     
    //LOOP TO CALCULATE THE FACTORIAL OF A NUMBER
    while(i<=n)
    {
        f=f*i;
        i++;
    }
     
    cout<<"\n The Factorial of "<<n<<" is "<<f;
    getch();
}
